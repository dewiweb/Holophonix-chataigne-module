{
	"name": "Holophonix",
	"type": "OSC",
	"path": "Spatial Audio",
	"version": "1.0",
	"description": "Chataigne Module specific documentation can be found in the README on GitHub: ",
	"url": "https://github.com/dewiweb/Holophonix_chataigne_module",
	"downloadURL": "https://github.com/dewiweb/Holophonix_chataigne_module",
	"hasInput": true,
	"hasOutput": true,
	"hideDefaultCommands": false,
	"hideDefaultParameters": [
		"colorSendMode",
		"autoRange",
		"autoFeedback",
		"pass_through",
		"input",
		"outputs",
		"useHierarchy",
		"colorSendMode",
		"booleanSendMode"
	],
	"defaults": {
		"autoAdd": false,
		"oscInput": {
			"localPort": 38033
		},
		"OSC Outputs": {
			"OSC Output": {
				"local": true,
				"remoteHost": "192.168.1.1",
				"remotePort": 9001
			}
		}
	},
	"parameters": {
		"Objects": {
			"type": "Container",
			"Objects IDs": {
				"description": "IDs of objects to add.\n\n Can be formatted as single Object(ex: 15), range(ex: 8-13) \n\n or list(ex: 2,4,12,20)\n",
				"type": "String"
			},
			"Add Objects": {
				"type": "Trigger"
			}
		},
		"Request Values": {
			"type": "Container",
			"Manual XYZ positions Request": {
				"type": "Trigger"
			},
			"Manual AED positions Request": {
				"type": "Trigger"
			},
			"Manual Gain Request": {
				"type": "Trigger"
			},
			"Auto XYZ positions Request": {
				"description": "Update automatically objects container values.\n",
				"type": "Boolean",
				"default": false
			},
			"Auto AED positions Request": {
				"description": "Update automatically objects container values.\n",
				"type": "Boolean",
				"default": false
			},
			"Auto Gain Request": {
				"description": "Update automatically objects container values.\n",
				"type": "Boolean",
				"default": false
			},
			" Auto Request Rate": {
				"description": "automatic Get request Rate in milliseconds",
				"type": "Integer",
				"default": 1000,
				"min": 1,
				"max": 10000
			}
		},
		"Record Cues": {
			"type": "Container",
			"Rec Mode": {
				"description": "ON : record position from Holo.\n\n OFF : send recorded positions to Holo.\n",
				"type": "Boolean",
				"default": true
			},
			"Create Global Cues": {
				"type": "Trigger"
			},
			"Global Cues Name": {
				"description": "Set a specific Name",
				"type": "String"
			},
			"Reload Cue": {
				"type": "Enum"
			}
		}
	},
	"scripts": [
		"Holophonix.js"
	],
	"values": {},
	"commands": {
		"Send xyz": {
			"menu": "Set objects",
			"callback": "xyz",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				},
				"xyz": {
					"type": "Point3D",
					"min": [
						-20,
						-20,
						-20
					],
					"max": [
						20,
						20,
						20
					]
				}
			}
		},
		"Send aed": {
			"menu": "Set objects",
			"callback": "aed",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				},
				"aed": {
					"type": "Point3D",
					"min": [
						-180,
						-90,
						0
					],
					"max": [
						180,
						90,
						20
					]
				}
			}
		},
		"Send gain": {
			"menu": "Set objects",
			"callback": "gain",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				},
				"gain": {
					"type": "Float",
					"min": -60,
					"max": 12
				}
			}
		},
		"Get xyz": {
			"menu": "Get objects",
			"callback": "getXYZ",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				}
			}
		},
		"Get aed": {
			"menu": "Get objects",
			"callback": "getAED",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				}
			}
		},
		"Get gain": {
			"menu": "Get objects",
			"callback": "getGain",
			"parameters": {
				"Source index": {
					"type": "Integer",
					"min": 1,
					"max": 128
				}
			}
		}
	}
}